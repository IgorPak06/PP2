def spy_game(nums):
    New_List = list()
    for i in range(len(nums)):
        if (nums[i] == 0 or nums[i] == 7):
            New_List.append(nums[i])
    if (New_List[0] == 0 and New_List[1] == 0 and New_List[2] == 7):
        return True
    else:
        return False
    
numbers = [1,2,4,0,0,7,5]
if (spy_game(numbers)):
    print("True")
else:
    print("False")